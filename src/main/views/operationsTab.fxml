<SplitPane dividerPositions="0.15, 0.7" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.OperationsController">
   <items>
      <AnchorPane>
         <children>
            <ListView fx:id="operationsList" layoutY="119.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <BorderPane fx:id="operationContainer">
         <bottom>
            <HBox fx:id="controlsContainer" alignment="CENTER" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <Label text="Open image using &quot;File -&gt; Open&quot;" BorderPane.alignment="CENTER" />
         </center></BorderPane>
      <BorderPane fx:id="contoursContainer" />
   </items>
